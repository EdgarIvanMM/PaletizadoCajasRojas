PROC tomarCaja()
    MoveJ homeCr,v500,z50,gripperIvan;
    MoveJ acercamiento, v500, z50, gripperIvan;
    !WaitTime 2;
    MoveL bajar, v500, z50, gripperIvan;
    WaitRob \ZeroSpeed;
	Set ABB_Scalable_IO_0_DO2; 
    WaitTime 1;
    MoveL subirSalir, v500, z50, gripperIvan;
ENDPROC

PROC seleccionarArreglo()
     TPReadFK reg1, "Selecciona el acomodo del paletizado", stEmpty, stEmpty, stEmpty, "Acomodo 1", "Acomodo 2";
    
    IF reg1=4 THEN 
        Caja_red;
        TPWrite "Paletizando acomodo 1";
    ELSE
        Caja_red2;
        TPWrite "Paletizando acomodo 2";
    ENDIF
ENDPROC

PROC preguntaReinicio()
    TPReadFK reg1, "Â¿Empezar de nuevo el paletizado?", stEmpty, stEmpty, stEmpty, "Yes", "No";
    
    IF reg1=4 THEN 
        main;
        TPWrite "Reiniciando paletizado";
    ELSE
        Stop;
        TPWrite "Paletizado finalizado";
    ENDIF
ENDPROC


